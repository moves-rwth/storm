cat >input <<EOF
/* This Büchi automaton is not terminal, because of the
   accepting edge between states 0 and 1. */
HOA: v1
States: 2
Start: 1
AP: 1 "a"
Acceptance: 1 Inf(0)
--BODY--
State: 0
[t] 0
State: 1
[0] 0 {0}
[!0] 1
--END--
/* This co-Büchi automaton not terminal for the same reason.  */
HOA: v1
States: 2
Start: 1
AP: 1 "a"
Acceptance: 1 Fin(0)
--BODY--
State: 0
[t] 0
State: 1
[0] 0 {0}
[!0] 1
--END--
/* This co-Büchi automaton is terminal.  */
HOA: v1
States: 2
Start: 1
AP: 1 "a"
Acceptance: 1 Fin(0)
--BODY--
State: 0
[t] 0
State: 1
[0] 0 {0}
[!0] 1 {0}
--END--
EOF

