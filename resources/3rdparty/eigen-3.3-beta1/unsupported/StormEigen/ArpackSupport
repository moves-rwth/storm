#ifndef STORMEIGEN_ARPACKSUPPORT_MODULE_H
#define STORMEIGEN_ARPACKSUPPORT_MODULE_H

#include <StormEigen/Core>

#include <StormEigen/src/Core/util/DisableStupidWarnings.h>

/** \defgroup ArpackSupport_Module Arpack support module
  *
  * This module provides a wrapper to Arpack, a library for sparse eigenvalue decomposition.
  *
  * \code
  * #include <StormEigen/ArpackSupport>
  * \endcode
  */

#include <StormEigen/SparseCholesky>
#include "src/Eigenvalues/ArpackSelfAdjointEigenSolver.h"

#include <StormEigen/src/Core/util/ReenableStupidWarnings.h>

#endif // STORMEIGEN_ARPACKSUPPORT_MODULE_H
